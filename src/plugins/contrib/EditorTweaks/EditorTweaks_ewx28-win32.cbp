<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="EditorTweaks" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../build28/share/EmBlocks/plugins/EditorTweaks" prefix_auto="1" extension_auto="1" />
				<Option working_dir="../../../build28" />
				<Option object_output="$(EB_OBJ)/plugins/contrib/EditorTweaks" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance --no-check-associations -ns -nd" />
				<Option host_application="../../../build28/emblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-g" />
					<Add option="-pipe" />
					<Add option="-mthreads" />
					<Add option="-fmessage-length=0" />
					<Add option="-fexceptions" />
					<Add option="-Winvalid-pch" />
					<Add option="-iquote$(EB_OBJ)/sdk/include" />
					<Add option='-include &quot;sdk.h&quot;' />
					<Add option="-DBUILDING_PLUGIN" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DCB_PRECOMP" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DcbDEBUG" />
					<Add directory="../../../sdk/include" />
					<Add directory="$(#ewx.include)" />
					<Add directory="$(#ewx.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
				</Compiler>
				<Linker>
					<Add library="emblocks" />
					<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
					<Add directory="../../../build28" />
					<Add directory="$(#ewx.lib)/gcc_dll$(WX_CFG)" />
				</Linker>
				<ExtraCommands>
					<Add after="zip -j9 ..\..\..\build28\share\EmBlocks\EditorTweaks.zip manifest.xml" />
				</ExtraCommands>
			</Target>
			<Environment>
				<Variable name="EB_OBJ" value="..\..\..\.objs28" />
				<Variable name="WX_CFG" value="_wxeb" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="28" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Unit filename="EditorTweaks.cpp" />
		<Unit filename="EditorTweaks.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
