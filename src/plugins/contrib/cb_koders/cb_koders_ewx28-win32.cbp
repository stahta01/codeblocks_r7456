<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Koders Query" />
		<Option pch_mode="0" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="../../../build28/share/emblocks/plugins/cb_koders.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="$(EB_OBJ)/plugins/contrib/cb_koders" />
				<Option external_deps="../../../build28/libemblocks.a;../../../build28/libwxscintilla_cb.a;" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd" />
				<Option host_application="../../../build28/emblocks.exe" />
				<Option run_host_application_in_terminal="0" />
				<Option projectResourceIncludeDirsRelation="0" />
			</Target>
			<Environment>
				<Variable name="EB_OBJ" value="../../../.objs28" />
				<Variable name="WX_CFG" value="_wxeb" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="28" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-pipe" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-iquote$(EB_OBJ)/sdk/include" />
			<Add option="-mthreads" />
			<Add option="-DcbDEBUG" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add directory="$(#EWX.include)" />
			<Add directory="$(#EWX.lib)/gcc_dll$(WX_CFG)/msw$(WX_SUFFIX)" />
			<Add directory="../../../sdk/include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#EWX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-Wl,--enable-auto-import" />
			<Add option="-mthreads" />
			<Add library="wxmsw$(WX_VERSION)$(WX_SUFFIX)" />
			<Add library="emblocks" />
			<Add directory="$(#EWX.lib)/gcc_dll$(WX_CFG)" />
			<Add directory="../../../build28" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\build28\share\emblocks\cb_koders.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="cb_koders.cpp" />
		<Unit filename="cb_koders.h" />
		<Unit filename="kodersdialog.cpp" />
		<Unit filename="kodersdialog.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<wxsmith version="1">
				<gui name="wxWidgets" src="" main="" init_handlers="necessary" language="CPP" />
				<resources>
					<wxScrollingDialog wxs="wxsmith/KodersDialog.wxs" src="kodersdialog.cpp" hdr="kodersdialog.h" name="KodersDialog" language="CPP" />
				</resources>
			</wxsmith>
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
