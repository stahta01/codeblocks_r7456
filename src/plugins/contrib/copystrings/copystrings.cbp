<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="CopyStrings" />
		<Option compiler="gcc" />
		<Build>
			<Target title="default">
				<Option output="..\..\..\build\share\EmBlocks\plugins\copystrings.dll" prefix_auto="0" extension_auto="0" />
				<Option object_output="..\..\..\foo\.objs" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option parameters="--debug-log --multiple-instance -na -ns -nd" />
				<Option host_application="..\..\..\build\codeblocks.exe" />
			</Target>
			<Environment>
				<Variable name="WX_CFG" value="_wxeb" />
				<Variable name="WX_SUFFIX" value="u" />
				<Variable name="WX_VERSION" value="1" />
			</Environment>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="default;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-g" />
			<Add option="-pipe" />
			<Add option="-mthreads" />
			<Add option="-fmessage-length=0" />
			<Add option="-fexceptions" />
			<Add option="-Winvalid-pch" />
			<Add option="-DBUILDING_PLUGIN" />
			<Add option="-DCB_PRECOMP" />
			<Add option="-DWX_PRECOMP" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DwxUSE_UNICODE" />
			<Add option="-DWXMAKINGDLL_PROPGRID" />
			<Add option="-DwxPG_USE_WXMODULE=0" />
			<Add directory="$(#EWX.include)" />
			<Add directory="$(#EWX.lib)\gcc_dll$(WX_CFG)\msw$(WX_SUFFIX)" />
			<Add directory="$(#EWX)\contrib\include" />
			<Add directory="..\..\..\sdk\include" />
			<Add directory="..\..\..\sdk\wxscintilla\include" />
		</Compiler>
		<ResourceCompiler>
			<Add directory="$(#WX.include)" />
		</ResourceCompiler>
		<Linker>
			<Add option="-mthreads" />
			<Add library="wxmsw$(WX_VERSION)" />
			<Add library="emblocks" />
			<Add directory="$(#EWX.lib)\gcc_dll$(WX_CFG)" />
			<Add directory="..\..\..\build" />
		</Linker>
		<ExtraCommands>
			<Add after="zip -j9 ..\..\..\build\share\EmBlocks\copystrings.zip manifest.xml" />
			<Mode after="always" />
		</ExtraCommands>
		<Unit filename="copystrings.cpp" />
		<Unit filename="copystrings.h" />
		<Unit filename="manifest.xml" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>