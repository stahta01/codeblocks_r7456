<?xml version="1.0" encoding="utf-8" ?>
<resource>
	<object class="wxScrollingDialog" name="dlgNewFromTemplate">
		<title>New from template</title>
		<centered>1</centered>
		<object class="wxFlexGridSizer">
			<cols>2</cols>
			<rows>2</rows>
			<vgap>8</vgap>
			<hgap>8</hgap>
			<growablecols>0</growablecols>
			<growablerows>0</growablerows>
			<object class="sizeritem">
				<object class="wxListbook" name="nbMain">
					<object class="listbookpage">
						<object class="wxPanel" name="pnlProjects">
							<object class="wxBoxSizer">
								<orient>wxVERTICAL</orient>
								<object class="sizeritem">
									<object class="wxBoxSizer">
										<object class="sizeritem">
											<object class="wxStaticText" name="ID_STATICTEXT1">
												<label>Category:</label>
											</object>
											<flag>wxTOP|wxALIGN_LEFT|wxALIGN_TOP</flag>
											<border>4</border>
										</object>
										<object class="sizeritem">
											<object class="wxBoxSizer">
												<orient>wxVERTICAL</orient>
												<object class="sizeritem">
													<object class="wxChoice" name="cmbProjectCategories" />
													<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
													<option>1</option>
												</object>
											</object>
											<flag>wxLEFT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
											<border>8</border>
											<option>1</option>
										</object>
									</object>
									<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
								</object>
								<object class="sizeritem">
									<object class="wxListCtrl" name="listProjects">
										<size>-1,250</size>
										<style>wxLC_ICON|wxLC_AUTOARRANGE|wxLC_SINGLE_SEL</style>
									</object>
									<flag>wxTOP|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
									<border>8</border>
									<option>1</option>
								</object>
							</object>
						</object>
						<label>Projects</label>
					</object>
					<object class="listbookpage">
						<object class="wxPanel" name="pnlFiles">
							<object class="wxBoxSizer">
								<orient>wxVERTICAL</orient>
								<object class="sizeritem">
									<object class="wxBoxSizer">
										<object class="sizeritem">
											<object class="wxStaticText" name="ID_STATICTEXT3">
												<label>Category:</label>
											</object>
											<flag>wxTOP|wxALIGN_LEFT|wxALIGN_TOP</flag>
											<border>4</border>
										</object>
										<object class="sizeritem">
											<object class="wxBoxSizer">
												<orient>wxVERTICAL</orient>
												<object class="sizeritem">
													<object class="wxChoice" name="cmbFileCategories" />
													<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
													<option>1</option>
												</object>
											</object>
											<flag>wxLEFT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
											<border>8</border>
											<option>1</option>
										</object>
									</object>
									<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
								</object>
								<object class="sizeritem">
									<object class="wxListCtrl" name="listFiles">
										<style>wxLC_ICON|wxLC_AUTOARRANGE|wxLC_SINGLE_SEL</style>
									</object>
									<flag>wxTOP|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
									<border>8</border>
									<option>1</option>
								</object>
							</object>
						</object>
						<label>Files</label>
					</object>
					<object class="listbookpage">
						<object class="wxPanel" name="pnlCustoms">
							<object class="wxBoxSizer">
								<orient>wxVERTICAL</orient>
								<object class="sizeritem">
									<object class="wxBoxSizer">
										<object class="sizeritem">
											<object class="wxStaticText" name="ID_STATICTEXT4">
												<label>Category:</label>
											</object>
											<flag>wxTOP|wxALIGN_LEFT|wxALIGN_TOP</flag>
											<border>4</border>
										</object>
										<object class="sizeritem">
											<object class="wxBoxSizer">
												<orient>wxVERTICAL</orient>
												<object class="sizeritem">
													<object class="wxChoice" name="cmbCustomCategories" />
													<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
													<option>1</option>
												</object>
											</object>
											<flag>wxLEFT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
											<border>8</border>
											<option>1</option>
										</object>
									</object>
									<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
								</object>
								<object class="sizeritem">
									<object class="wxListCtrl" name="listCustoms">
										<style>wxLC_ICON|wxLC_AUTOARRANGE|wxLC_SINGLE_SEL</style>
									</object>
									<flag>wxTOP|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
									<border>8</border>
									<option>1</option>
								</object>
							</object>
						</object>
						<label>Custom</label>
					</object>
					<object class="listbookpage">
						<object class="wxPanel" name="pnlUser">
							<object class="wxBoxSizer">
								<orient>wxVERTICAL</orient>
								<object class="sizeritem">
									<object class="wxListBox" name="lstUser" />
									<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
									<option>1</option>
								</object>
								<object class="sizeritem">
									<object class="wxStaticText" name="ID_STATICTEXT5">
										<label>* To create a user template, create a project the way you want it&#x0D;&#x0A;   and click &quot;Project-&gt;Save project as user-template&quot; in main menu bar.</label>
									</object>
									<flag>wxBOTTOM|wxALIGN_LEFT|wxALIGN_TOP</flag>
									<border>8</border>
								</object>
								<object class="sizeritem">
									<object class="wxStaticText" name="ID_STATICTEXT6">
										<label>* To delete a user template, delete the similarly named folder created&#x0D;&#x0A;   in %APPDATA%\\emblocks\\UserTemplates (for Windows) or&#x0D;&#x0A;   ~/Library/Application Support/EmBlocks (for Mac OS X) or&#x0D;&#x0A;   ~/.emblocks (for other platforms).</label>
									</object>
									<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
								</object>
								<object class="sizeritem">
									<object class="wxStaticText" name="ID_STATICTEXT7">
										<label>   Please note the dot (.) in front of &quot;emblocks&quot;!</label>
										<fg>#800000</fg>
									</object>
									<flag>wxALIGN_LEFT|wxALIGN_TOP</flag>
								</object>
							</object>
						</object>
						<label>User templates</label>
					</object>
				</object>
				<flag>wxTOP|wxLEFT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>8</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<object class="wxButton" name="wxID_OK">
							<label>&amp;Go</label>
							<default>1</default>
						</object>
						<flag>wxBOTTOM|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
						<border>8</border>
					</object>
					<object class="sizeritem">
						<object class="wxButton" name="wxID_CANCEL">
							<label>&amp;Cancel</label>
						</object>
						<flag>wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
					</object>
					<object class="spacer">
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<option>1</option>
						<size>118,46</size>
					</object>
					<object class="sizeritem">
						<object class="wxRadioBox" name="rbView">
							<label>View as</label>
							<content>
								<item>Large icons</item>
								<item>List</item>
							</content>
							<style>wxRA_SPECIFY_COLS</style>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
					</object>
				</object>
				<flag>wxTOP|wxRIGHT|wxEXPAND|wxALIGN_TOP|wxALIGN_CENTER_HORIZONTAL</flag>
				<border>8</border>
			</object>
			<object class="sizeritem">
				<object class="wxBoxSizer">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT8">
							<label>TIP: Try right-clicking an item</label>
							<fg>#0000FF</fg>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
					</object>
					<object class="sizeritem">
						<object class="wxStaticLine" name="ID_STATICLINE1" />
						<flag>wxTOP|wxBOTTOM|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
						<border>8</border>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT9">
							<label>1. Select a wizard type first on the left</label>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT10">
							<label>2. Select a specific wizard from the main window (filter by categories if needed)</label>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
					</object>
					<object class="sizeritem">
						<object class="wxStaticText" name="ID_STATICTEXT11">
							<label>3. Press Go</label>
						</object>
						<flag>wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
					</object>
				</object>
				<flag>wxBOTTOM|wxLEFT|wxEXPAND|wxALIGN_LEFT|wxALIGN_TOP</flag>
				<border>8</border>
			</object>
		</object>
	</object>
</resource>
